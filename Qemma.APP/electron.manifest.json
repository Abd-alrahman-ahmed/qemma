{
  "executable": "Qemma.APP",
  "splashscreen": {
    "imageFile": "./wwwroot/splashscreen.png"
  },
  "name": "Al-Qemma",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.qemma.app",
    "productName": "Al-Qemma",
    "copyright": "Copyright Â© 2022",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "../../../wwwroot/favicon.ico"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}